// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model shortenedUrls {
  short_id  String @unique @id
  full_url  String @unique
  short_url String @unique
}
